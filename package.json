{
  "name": "3d-models",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --bun run next dev",
    "dev:inspect": "bun --bun run next dev --inspect",
    "next:upgrade": "next upgrade",
    "next:analyze": "next experimental-analyze",
    "build": "bun --bun run next build",
    "build:debug": "bun --bun run next build --debug-prerender",
    "start": "bun --bun run next start",
    "typegen": "next typegen && tsgo --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "bun --conditions=react-server src/db/seed.ts",
    "db:drop": "bun --conditions=react-server src/db/drop-tables.ts",
    "prepare": "husky",
    "type": "tsgo --noEmit --skipLibCheck --project tsconfig.json",
    "fix": "ultracite fix",
    "check": "ultracite check",
    "doctor": "ultracite doctor",
    "ultracite:upgrade": "bun x ultracite@latest init --pm bun --linter biome --agents cursor-cli --editors vscode --hooks cursor --frameworks next --quiet"
  },
  "dependencies": {
    "elysia": "1.4.24",
    "@elysiajs/openapi": "^1.4.14",
    "@elysiajs/cors": "^1.4.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "better-auth": "beta",
    "clsx": "^2.1.1",
    "drizzle-orm": "beta",
    "@neondatabase/serverless": "^1.0.2",
    "next": "^16.1.6",
    "nuqs": "^2.8.8",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-icons": "^5.5.0",
    "server-only": "^0.0.1",
    "slugify": "^1.6.6",
    "valibot": "^1.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.13",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@toolwind/corner-shape": "^0.0.8-3",
    "@types/bun": "^1.3.9",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@typescript/native-preview": "^7.0.0-dev.20260210.1",
    "drizzle-kit": "beta",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "next-devtools-mcp": "^0.3.10",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-plugin-directives": "^0.0.3",
    "ultracite": "7.1.5"
  },
  "trustedDependencies": [
    "@prisma/client",
    "@prisma/engines",
    "@swc/core",
    "@tailwindcss/oxide",
    "esbuild",
    "prisma",
    "sharp"
  ],
  "overrides": {
    "tmp": ">=0.2.4",
    "glob": "^12.0.0",
    "body-parser": "^2.2.1",
    "express": "^5.2.0",
    "@modelcontextprotocol/sdk": ">1.25.3",
    "jws": "^3.2.3",
    "@trpc/server": ">=11.8.0",
    "qs": ">=6.14.1",
    "@sinclair/typebox": "0.34.46",
    "undici": ">=7.18.2",
    "hono": ">=4.11.7",
    "lodash": ">4.17.22",
    "@isaacs/brace-expansion": ">5.0.0"
  }
}
