{
  "name": "3d-models",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --bun run next dev",
    "dev:inspect": "bun --bun run next dev --inspect",
    "next:upgrade": "next upgrade",
    "next:analyze": "next experimental-analyze",
    "build": "bun --bun run next build",
    "build:debug": "bun --bun run next build --debug-prerender",
    "start": "bun --bun run next start",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:unit": "bun test tests/unit",
    "test:components": "bun test tests/components",
    "test:integration": "bun test tests/integration",
    "test:e2e": "playwright test",
    "e2e:open": "playwright test --ui",
    "e2e:codegen": "playwright codegen http://localhost:3000",
    "typegen": "next typegen && tsgo --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "bun --conditions=react-server src/db/seed.ts",
    "db:drop": "bun --conditions=react-server src/db/drop-tables.ts",
    "prepare": "husky",
    "type": "tsgo --noEmit --skipLibCheck --project tsconfig.json",
    "fix": "ultracite fix",
    "check": "ultracite check",
    "doctor": "ultracite doctor",
    "ultracite:upgrade": "bun x ultracite@latest init --pm bun --linter biome --agents cursor-cli --editors vscode --hooks cursor --frameworks next --quiet"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.1",
    "@elysiajs/openapi": "^1.4.14",
    "@neondatabase/serverless": "^1.0.2",
    "babel-plugin-react-compiler": "^1.0.0",
    "better-auth": "beta",
    "clsx": "^2.1.1",
    "drizzle-orm": "beta",
    "elysia": "^1.4.26",
    "next": "^16.1.6",
    "nuqs": "^2.8.8",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-icons": "^5.5.0",
    "server-only": "^0.0.1",
    "slugify": "^1.6.6",
    "valibot": "^1.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.4.0",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4.2.1",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@toolwind/corner-shape": "^0.0.8-3",
    "@types/bun": "^1.3.9",
    "@types/node": "^25.3.1",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript/native-preview": "^7.0.0-dev.20260225.1",
    "drizzle-kit": "beta",
    "happy-dom": "^20.7.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "next-devtools-mcp": "^0.3.10",
    "tailwindcss": "^4.2.1",
    "typescript": "^6.0.0-dev.20260225",
    "typescript-plugin-directives": "^0.0.3",
    "ultracite": "7.2.3"
  },
  "trustedDependencies": [
    "@prisma/client",
    "@prisma/engines",
    "@swc/core",
    "@tailwindcss/oxide",
    "esbuild",
    "prisma",
    "sharp"
  ],
  "overrides": {
    "tmp": ">=0.2.4",
    "glob": "^12.0.0",
    "body-parser": "^2.2.1",
    "express": "^5.2.0",
    "@modelcontextprotocol/sdk": ">1.25.3",
    "jws": "^3.2.3",
    "@trpc/server": ">=11.8.0",
    "qs": ">6.14.1",
    "@sinclair/typebox": "0.34.46",
    "undici": ">=7.18.2",
    "hono": ">=4.12.2",
    "lodash": ">4.17.22",
    "@isaacs/brace-expansion": ">5.0.0",
    "ajv": ">=8.18.0",
    "minimatch": ">=10.2.1"
  }
}
