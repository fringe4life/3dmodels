{
  "name": "3d-models",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:inspect": "next dev --inspect",
    "next:upgrade": "next upgrade",
    "next:analyze": "next analyze",
    "build": "next build",
    "build:debug": "next build --debug-prerender",
    "start": "next start",
    "lint": "biome lint src",
    "lint:fix": "biome check --write src",
    "lint:unsafe": "biome check --write --unsafe src",
    "format": "biome format --write src",
    "typegen": "next typegen && tsgo",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/db/seed.ts",
    "db:drop": "tsx src/db/drop-tables.ts",
    "prepare": "husky",
    "type": "tsgo --noEmit --skipLibCheck --project tsconfig.json",
    "ultracite": "bun x ultracite",
    "ultracite:fix": "bun x ultracite fix",
    "ultracite:check": "bun x ultracite check",
    "ultracite:doctor": "bun x ultracite doctor",
    "ultracite:upgrade": "bun x ultracite@latest init --pm bun --linter biome --agents cursor-cli --editors vscode --hooks cursor --frameworks next --quiet"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.1",
    "@neondatabase/serverless": "^1.0.2",
    "@toolwind/corner-shape": "^0.0.8-3",
    "babel-plugin-react-compiler": "^1.0.0",
    "better-auth": "^1.4.10",
    "bufferutil": "^4.1.0",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "beta",
    "drizzle-typebox": "^0.3.3",
    "elysia": "^1.4.21",
    "next": "^16.1.1",
    "next-devtools-mcp": "^0.3.9",
    "nuqs": "^2.8.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-icons": "^5.5.0",
    "server-only": "^0.0.1",
    "slugify": "^1.6.6",
    "typescript-plugin-directives": "^0.0.3",
    "uuid": "^13.0.0",
    "valibot": "^1.2.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@types/minimatch": "^6.0.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.18.1",
    "@typescript/native-preview": "^7.0.0-dev.20260106.1",
    "drizzle-kit": "beta",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "ultracite": "7.0.8"
  },
  "trustedDependencies": [
    "@swc/core",
    "@tailwindcss/oxide",
    "bufferutil",
    "esbuild",
    "sharp"
  ],
  "overrides": {
    "esbuild": ">=0.25",
    "tmp": ">=0.2.4",
    "glob": "^12.0.0",
    "body-parser": "^2.2.1",
    "express": "^5.2.0",
    "@modelcontextprotocol/sdk": ">=1.24.0",
    "jws": "^3.2.3",
    "@trpc/server": ">=11.8.0",
    "qs": ">=6.14.1",
    "@sinclair/typebox": "0.34.46"
  }
